<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout 
	android:id="@+id/RelativeLayout01" 
	android:layout_width="fill_parent" 
	xmlns:android="http://schemas.android.com/apk/res/android" 
	android:layout_height="wrap_content" 
	android:paddingBottom="4dip" 
	android:paddingLeft="12dip"
	android:paddingRight="12dip">
	<TextView 
		android:textSize="24dip" 
	    android:layout_width="wrap_content" 
	    android:layout_height="wrap_content" 
	    android:id="@+id/ItemHelpInvoiceNumber"
	    android:layout_below="@+id/ItemHelpDateTime"
	    android:textColor="#FFFFFF"
	    />
	<TextView 
		android:textSize="24dip" 
	    android:layout_width="wrap_content" 
	    android:layout_height="wrap_content" 
	    android:id="@+id/ItemHelpInvoiceNumberLight"
	    android:layout_toRightOf="@+id/ItemHelpInvoiceNumber"
	    android:layout_below="@+id/ItemHelpDateTime"
	    android:textColor="#FF9900"
	    />
	<TextView 
		android:textSize="24dip" 
	    android:layout_width="wrap_content" 
	    android:layout_height="wrap_content" 
	    android:id="@+id/ItemHelpMoney"
	    android:layout_toLeftOf="@+id/ItemHelpCheck"
	    android:textColor="#FF5500"
	    />
	
	<TextView 
	    android:paddingTop="12dip"
	    android:layout_height="wrap_content" 
	    android:textSize="12dip" 
	    android:layout_width="wrap_content" 
	    android:id="@+id/ItemHelpDateTime"
	    />
	<ImageView       
    	android:layout_width="wrap_content"   
    	android:layout_height="wrap_content"
    	android:id="@+id/ItemHelpStatus"  
    	android:layout_toRightOf="@+id/ItemHelpInvoiceNumberLight"
    	android:layout_below="@+id/ItemHelpDateTime"
    /> 
    <CheckBox 
		android:layout_alignParentRight="true" 
		android:textSize="24dip"
		android:layout_width="wrap_content" 
		android:layout_height="wrap_content" 
	    android:id="@+id/ItemHelpCheck"
	    android:textColor="#FF9900"
	    />
</RelativeLayout>