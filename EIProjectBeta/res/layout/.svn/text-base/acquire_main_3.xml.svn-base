<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent">
			<TextView 
				android:layout_centerHorizontal="True"
				android:textSize="36dip" 
			    android:layout_width="wrap_content" 
			    android:layout_height="wrap_content" 
			    android:textColor="#FF0099"
			    android:text="領獎明細"
			    />
			<TextView 
				android:paddingBottom="10dip" 
				android:paddingTop="56dip"
			    android:layout_height="wrap_content" 
			    android:textSize="24dip" 
			    android:layout_width="wrap_content" 
			    android:text="日期:"
			    android:id="@+id/AcquireDateTimeLabel"
			    />
			<TextView 
				android:paddingBottom="10dip" 
				android:paddingTop="56dip"
			    android:layout_height="wrap_content" 
			    android:textSize="24dip" 
			    android:layout_width="wrap_content" 
			    android:id="@+id/AcquireDialogItemDateTime"
			    android:layout_toRightOf="@+id/AcquireDateTimeLabel"
			    />
			<TextView 
				android:paddingBottom="10dip" 
			    android:layout_height="wrap_content" 
			    android:textSize="24dip"
			    android:layout_width="wrap_content" 
		        android:layout_below="@+id/AcquireDialogItemDateTime" 
			    android:text="總金額:"
			    android:id="@+id/AcquireTotalMoneyLabel"
			    />
			<TextView 
				android:paddingBottom="10dip" 
				android:layout_height="wrap_content" 
				android:layout_width="wrap_content" 
			    android:textSize="24dip"
			    android:id="@+id/AcquireDialogItemTotalMoney"
			    android:layout_below="@+id/AcquireDialogItemDateTime" 
				android:layout_toRightOf="@+id/AcquireTotalMoneyLabel"
			    />
			<TextView 
				android:paddingBottom="10dip" 
			    android:layout_height="wrap_content" 
			    android:textSize="24dip" 
			    android:layout_width="wrap_content" 
		        android:layout_below="@+id/AcquireDialogItemTotalMoney" 
			    android:text="稅後金額:"
			    android:id="@+id/AcquireTaxMoney"
			    />
			<TextView 
				android:paddingBottom="10dip" 
				android:layout_height="wrap_content" 
				android:layout_width="wrap_content" 
			    android:textSize="24dip"
			    android:id="@+id/AcquireDialogItemTaxMoney"
			    android:layout_below="@+id/AcquireDialogItemTotalMoney"
			    android:layout_toRightOf="@+id/AcquireTaxMoney"/>
</RelativeLayout>
