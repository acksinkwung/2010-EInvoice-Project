<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout 
	android:id="@+id/RelativeLayout01" 
	android:layout_width="fill_parent" 
	xmlns:android="http://schemas.android.com/apk/res/android" 
	android:layout_height="wrap_content" 
	android:paddingBottom="4dip" 
	android:paddingLeft="12dip"
	android:paddingRight="12dip">
	<TextView 
		android:textSize="24dip" 
	    android:layout_width="wrap_content" 
	    android:layout_height="wrap_content" 
	    android:id="@+id/ItemHelpHistoryDetailInvoiceNumber"
	    android:layout_below="@+id/ItemHelpHistoryDetailDateTime"
	    android:textColor="#FFFFFF"
	    />
	<TextView 
		android:textSize="24dip" 
	    android:layout_width="wrap_content" 
	    android:layout_height="wrap_content" 
	    android:id="@+id/ItemHelpHistoryDetailInvoiceNumberLight"
	    android:layout_toRightOf="@+id/ItemHelpHistoryDetailInvoiceNumber"
	    android:layout_below="@+id/ItemHelpHistoryDetailDateTime"
	    android:textColor="#FF9900"
	    />
	<TextView 
		android:textSize="36dip" 
	    android:layout_width="wrap_content" 
	    android:layout_height="wrap_content" 
	    android:id="@+id/ItemHelpHistoryDetailMoney"
	    android:layout_alignParentRight="true"
	    android:textColor="#FF5500"
	    />
	
	<TextView 
	    android:paddingTop="12dip"
	    android:layout_height="wrap_content" 
	    android:textSize="12dip" 
	    android:layout_width="wrap_content" 
	    android:id="@+id/ItemHelpHistoryDetailDateTime"
	    />
	<ImageView       
    	android:layout_width="wrap_content"   
    	android:layout_height="wrap_content"
    	android:id="@+id/ItemHelpHistoryDetailStatus"  
    	android:layout_toRightOf="@+id/ItemHelpHistoryDetailInvoiceNumberLight"
    	android:layout_below="@+id/ItemHelpHistoryDetailDateTime"
    /> 
</RelativeLayout>